float GetDensityHighY(int2 transVoxelCoord, float3 inDirection)
{
    return transVoxelCoord.x < voxelsPerAxis ?
        testBuffer[GetIsoIndex(GetDirectionalIsoIndex(transVoxelCoord, (inDirection * -1)))] : // C
        testBuffer[GetIsoIndex(GetDirectionalIsoIndex((transVoxelCoord - int2(voxelsPerAxis, 0)), (inDirection * -1)))]; // D
}

float GetDensityLowY(int2 transVoxelCoord, float3 inDirection)
{
    return transVoxelCoord.x < voxelsPerAxis ? 
        testBuffer[GetIsoIndex(GetDirectionalIsoIndex(transVoxelCoord, (inDirection * -1)))] : // A
        testBuffer[GetIsoIndex(GetDirectionalIsoIndex((transVoxelCoord - int2(voxelsPerAxis, 0)), (inDirection * -1)))]; // B
}

// presumes >= 8 coord
float GetTransvoxelLowRes(int2 voxelCoord, int transVoxelCoord, int3 inDirection)
{
    voxelCoord.x += (offsets[transVoxelCoord].x / 2);
    voxelCoord.y += (offsets[transVoxelCoord].y / 2);

    int3 pos = GetDirectionalIsoIndex(voxelCoord, inDirection);
    bool isEdge = any(pos < 0) || any(pos > voxelsPerAxis);
    
    return isEdge ? 
        isoLevel : 
        isoValues[GetIsoIndex(pos)]; 
}

// presumes < 9 coord
float GetTransvoxelHighRes(int2 voxelCoord, int transVoxelCoord, int3 inDirection)
{    
    voxelCoord.x = (voxelCoord.x * 2);
    voxelCoord.y = (voxelCoord.y * 2);
    
    voxelCoord.x += offsets[transVoxelCoord].x;
    voxelCoord.y += offsets[transVoxelCoord].y;

    return voxelCoord.y >= voxelsPerAxis ?
            GetDensityHighY((voxelCoord - int2(0, (voxelsPerAxis))), inDirection) :
            GetDensityLowY(voxelCoord, inDirection);
}

float GetTransvoxelDensity(int2 voxelCoord, int transVoxelCoord, int3 inDirection)
{
    return (transVoxelCoord >= 9) ? 
        GetTransvoxelLowRes(voxelCoord, transVoxelCoord, inDirection) : 
        GetTransvoxelHighRes(voxelCoord, transVoxelCoord, inDirection);
}

// Needs refactor
int GetType(int3 coord)
{
    // This is a transvoxel coordinate in 3d- workings below are not accurate
    bool isEdge = any(coord < 0) || any(coord > voxelsPerAxis);
    return isEdge ? 0 : typeValues[(GetIsoIndex(coord))];
}

float3 InterpolateVertex(uint edgeCode, int3 cornerCoords[13], float cornerValues[13], float3 worldPos)
{
    uint a = (edgeCode >> 4) & 0x0F;
    uint b = edgeCode & 0x0F; //0-15
    
    float3 pa = pa < 13 ? cornerCoords[a] : float3(0,0,0); // + worldPos
    float3 pb = pa < 13 ? cornerCoords[b] : float3(0,0,0);

    float da = cornerValues[a];
    float db = cornerValues[b];
    
    float dab = abs(da - db);
    float t = dab == 0 ? 0 : da / dab;
    return lerp(pa, pb, t);
}

// x, y, 0
// Use id as int2 as z value is 0. 
[numthreads(THREADS_X, THREADS_Y, THREADS_Z)]
void TransvoxelMarchingCubes(int3 id : SV_DispatchThreadID)
{
    if (any(id >= voxelsPerAxis))
        return;
    
    if (id.z > 0)
        return;
    
    int transitionCellMarchIndex = (id.x * voxelsPerAxis) + id.y;
    int maxedCount = (voxelsPerAxis * voxelsPerAxis * voxelsPerAxis) * 15;
    int totalMarchCellsPerTransitionCell = voxelsPerAxis * voxelsPerAxis;
    int flatIndex = (transitionCellIndex * totalMarchCellsPerTransitionCell) + transitionCellMarchIndex;
    int vertexStartIndex = maxedCount + (flatIndex * 36);
    
    float scale = baseDepthScale / (1 << leafDepth); // 400/(2 or 4) => 200 or 100
    float centerDis = scale / 2.0; // 100, 50
    float isoScale = (scale / (voxelsPerAxis)); // 10 or 5
    float3 minimumCornerWorldPos = leafPosition - float3(centerDis, centerDis, centerDis);
    int2 transVoxelCoord = int2(id.x, id.y);
    
    float3 inDir = direction;
    inDir.x *= -1;
    inDir.y *= -1;
    inDir.z *= -1;

    float _trCellValues[13];
    
    //for (int i = 0; i < 13; i++)
    //_trCellValues[i] = GetTransvoxelDensity(transVoxelCoord, i, inDir);
    
    _trCellValues[0] = GetTransvoxelDensity(transVoxelCoord, 0, inDir);
    _trCellValues[2] = GetTransvoxelDensity(transVoxelCoord, 2, inDir);
    _trCellValues[6] = GetTransvoxelDensity(transVoxelCoord, 6, inDir);
    _trCellValues[8] = GetTransvoxelDensity(transVoxelCoord, 8, inDir);

    _trCellValues[1] = GetTransvoxelDensity(transVoxelCoord, 1, inDir);
    _trCellValues[3] = GetTransvoxelDensity(transVoxelCoord, 3, inDir);
    _trCellValues[4] = GetTransvoxelDensity(transVoxelCoord, 4, inDir);
    _trCellValues[5] = GetTransvoxelDensity(transVoxelCoord, 5, inDir);
    _trCellValues[7] = GetTransvoxelDensity(transVoxelCoord, 7, inDir);
    
    _trCellValues[9] = _trCellValues[0]; //GetTransvoxelDensity(transVoxelCoord, 9, inDir);
    _trCellValues[10] = _trCellValues[2]; //GetTransvoxelDensity(transVoxelCoord, 10, inDir);
    _trCellValues[11] = _trCellValues[6]; //GetTransvoxelDensity(transVoxelCoord, 11, inDir);
    _trCellValues[12] = _trCellValues[8]; //GetTransvoxelDensity(transVoxelCoord, 12, inDir);

    /*_trCellValues[0x9] = _trCellValues[0];
    _trCellValues[0xA] = _trCellValues[2];
    _trCellValues[0xB] = _trCellValues[6];
    _trCellValues[0xC] = _trCellValues[8];*/
    
    int caseCode = ((
        _trCellValues[0] < isoLevel ? 1 : 0)
        | (_trCellValues[1] < isoLevel ? 2 : 0)
        | (_trCellValues[2] < isoLevel ? 4 : 0)
        | (_trCellValues[5] < isoLevel ? 8 : 0)
        | (_trCellValues[8] < isoLevel ? 16 : 0)
        | (_trCellValues[7] < isoLevel ? 32 : 0)
        | (_trCellValues[6] < isoLevel ? 64 : 0)
        | (_trCellValues[3] < isoLevel ? 128 : 0)
        | (_trCellValues[4] < isoLevel ? 256 : 0));
    
    if (caseCode == 0 || caseCode == 511)
        return;
    
    int3 voxelCoord = GetDirectionalIsoIndex(transVoxelCoord, inDir);
    int3 cornerCoords[13];
    
    for (int l = 0; l < 13; ++l)
        cornerCoords[l] = offsets[l];
    
    uint offset = transitionOffsets[caseCode];
    uint length = transitionLengths[caseCode]; //0-511
    
    for (int j = 0; j < 36; j++)
    {
        uint vertexFloatIndex = (vertexStartIndex + j) * 3;
        outVertexInfo[vertexFloatIndex] = 0.0;
        outVertexInfo[vertexFloatIndex + 1] = 0.0;
        outVertexInfo[vertexFloatIndex + 2] = 0.0;
        outTypeInfo[vertexStartIndex + j] = 0;
    }

    for (uint k = 0; k < length; k++) // Up to 0-24 
    {
        uint v0 = transitionLookup[offset + k]; //transitionLookup: size = 4752
        uint vertexFloatIndex = (vertexStartIndex + k) * 3;
        uint edgeCode = flatTransitionVertexData[caseCode * 12 + k];

        uint a = (edgeCode >> 4) & 0x0F;
        uint b = edgeCode & 0x0F;

        float3 pa = pa < 13 ? float3(cornerCoords[a].x/2, cornerCoords[a].y/2, cornerCoords[a].z/2) : float3(0, 0, 0);
        float3 pb = pa < 13 ? float3(cornerCoords[b].x/2, cornerCoords[b].y/2, cornerCoords[b].z/2) : float3(0, 0, 0);
        
        float densityA = _trCellValues[a];
        float densityB = _trCellValues[b];
    
        float denom = densityB - densityA; // 1 to -1
        float t = denom == 0 ? 0 : (isoLevel - densityA) / denom;
        float3 position = pa + (clamp(t, 0, 1) * (pb - pa));
        
        position.x *= abs(inDir.x) > 0.1 ? -inDir.x : 1.0;
        position.y *= abs(inDir.y) > 0.1 ? -inDir.y : 1.0;
        position.z *= abs(inDir.z) > 0.1 ? -inDir.z : 1.0;
        
        
        position += voxelCoord;
        position = minimumCornerWorldPos + (position * isoScale);
        
        outVertexInfo[vertexFloatIndex] = position.x;
        outVertexInfo[vertexFloatIndex + 1] = position.y;
        outVertexInfo[vertexFloatIndex + 2] = position.z;
        outTypeInfo[vertexStartIndex + k] = 0;
    }
}


bool DebugTriangleOnCondition(int a, int b, int k, int vertexFloatIndex, int vertexStartIndex)
{ 
    //  if (length % 3 != 0) // Length count check
    if (a > 12 || b > 12) // EdgeCode index check
    {
        float size = 200.0;
        float2 A = float2(-1.0, 1.0);
        float2 B = float2(1.0, 1.0);
        float2 C = float2(-1.0, -1.0);
        float2 D = float2(1.0, -1.0);
        
        float x = k % 3 == 0 ? A.x : k % 3 == 1 ? B.x : C.x;
        float y = k % 3 == 0 ? A.y : k % 3 == 1 ? B.y : C.y;

        if (k % 3 == 0 && k > 2)  
        /* Triangle check:
        outVertexInfo[vertexFloatIndex - 3] == p0.x &&
        outVertexInfo[vertexFloatIndex - 2] == p0.y &&
        outVertexInfo[vertexFloatIndex - 1] == p0.z &&
        outVertexInfo[vertexFloatIndex - 6] == p0.x &&
        outVertexInfo[vertexFloatIndex - 5] == p0.y &&
        outVertexInfo[vertexFloatIndex - 4] == p0.z)*/
        {
            outVertexInfo[vertexFloatIndex] = x * size;
            outVertexInfo[vertexFloatIndex + 1] = y * size;
            outVertexInfo[vertexFloatIndex + 2] = 0.0;
            outTypeInfo[vertexStartIndex + k] = 1;
            return true;

        }
    }
    return false;
}
        