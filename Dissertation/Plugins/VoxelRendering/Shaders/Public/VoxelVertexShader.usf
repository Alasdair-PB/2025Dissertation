#include "/Engine/Public/Platform.ush"
#include "VoxelPixelShader.usf"
#include "/VertexFactoryShaders/VoxelVertexFactory.ush"

struct VS_OUTPUT
{
    float4 Position : SV_POSITION;
};

VS_OUTPUT MainVS(FPositionOnlyVertexFactoryInput input)
{
    VS_OUTPUT output;
    output.Position = float4(input.Position,1);
    return output;
}
